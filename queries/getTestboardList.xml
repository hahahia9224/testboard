<?xml version="1.0" encoding="utf-8"?>
<query id="getTestboardList" action="select">
    <tables>
        <table name="testboard" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <navigation>
        <index var="sort_index" default="board_srl" order="desc" />
        <list_count var="list_count" default="5" />
        <page_count var="page_count" default="10" />
        <page var="page" default="1" />
    </navigation>
</query>
